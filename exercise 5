for i in range(10, 0 , -1):
  print("#" * i)

  x= 5
for i in range(1, x + 1):
    s = " " * (x - i)      
    h = "#" * (2 * i - 1)       
    print(s + h)           

x = 5

for i in range(1, x+ 1):
    print(" " * (x- i) + "#" * (2 * i - 1))

for i in range(x - 1, 0, -1):
    print(" " * (x- i) + "#" * (2 * i - 1))
