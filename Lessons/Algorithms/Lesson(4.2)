def trim(word):
    return word.rstrip('\n') 

def swap(B, p, q):
    temp = B[p]
    B[p] = B[q]
    B[q] = temp

def iSort(Z): 
    n = len(Z)
    for i in range(1, n):
        for j in range(0, i):
            if Z[i] < Z[j]:
                swap(Z, i, j)

def printIt(A):  
    for i in range(0, len(A), 10):
        print(" ".join(A[i:i+10]))

words = []
with open('words40.dat', 'r') as file:
    for line in file:
        trimmed = trim(line)
        words.append(trimmed)

iSort(words)
printIt(words)
print(f"{len(words)} words.")
